                    </Canvas>
                </Viewbox>
                <StackPanel Orientation="Horizontal" Margin="10,0,0,0">
                    <TextBlock Text="{Binding CurrentUserName, ElementName=Admin, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" FontFamily="/ControlSystemsLibrary;component/Resources/Fonts/#Roboto Condensed" FontSize="13">
                        <TextBlock.Foreground>
                            <SolidColorBrush Color="{DynamicResource Dark-003}"/>
                        </TextBlock.Foreground>
                    </TextBlock>
                    <TextBlock Text=" - " VerticalAlignment="Center" FontFamily="/ControlSystemsLibrary;component/Resources/Fonts/#Roboto Condensed" FontSize="13">
                        <TextBlock.Foreground>
                            <SolidColorBrush Color="{DynamicResource Dark-003}"/>
                        </TextBlock.Foreground>
                    </TextBlock>
                    <TextBlock Text="{Binding CurrentConnectionName, ElementName=Admin, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" FontFamily="/ControlSystemsLibrary;component/Resources/Fonts/#Roboto Condensed" FontSize="13">
                        <TextBlock.Foreground>
                            <SolidColorBrush Color="{DynamicResource Dark-003}"/>
                        </TextBlock.Foreground>
                    </TextBlock>
                </StackPanel>
            </StackPanel>
        </Button>
        <Grid x:Name="WorkGrid" MouseMove="MainGrid_MouseMove" Margin="66,0,0,0" Grid.ColumnSpan="2">
            <TabControl x:Name="AdminTabControl" BorderThickness="{Binding TabControlBorderThickness, ElementName=Admin, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding TabItemsCollection, ElementName=Admin, UpdateSourceTrigger=PropertyChanged}" Style="{DynamicResource AdminTabControlStyle}" Padding="0" SizeChanged="AdminTabControlSizeChanged" Loaded="AdminTabControlLoaded">
                <TabControl.BorderBrush>
                    <SolidColorBrush Color="{DynamicResource Light-005}"/>
                </TabControl.BorderBrush>
                <TabControl.Background>
                    <SolidColorBrush Color="{DynamicResource Light-002}"/>
                </TabControl.Background>
            </TabControl>
        </Grid>

        <Grid x:Name="MenuGrid" HorizontalAlignment="Left" Width="310" Margin="0" MouseEnter="Menu_MouseEnter" MouseLeave="Menu_MouseLeave" MouseMove="MenuMouseMove" SizeChanged="Menu_SizeChanged">
            <Grid.Background>
                <SolidColorBrush Color="{DynamicResource Light-001}"/>
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="65"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Rectangle Grid.Column="1" HorizontalAlignment="Right" Width="1" Visibility="Hidden">
                <Rectangle.Fill>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="{DynamicResource Light-002}" Offset="0"/>
                        <GradientStop Color="{DynamicResource Light-002}" Offset="1"/>
                        <GradientStop Color="{DynamicResource Blue-003}" Offset="0.5"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
            <Border>
                <Border.Background>
                    <SolidColorBrush Color="{DynamicResource Light-001}"/>
                </Border.Background>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="32"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <CheckBox x:Name="MenuStateCheckBox" Style="{DynamicResource CheckBoxStyle2}" SnapsToDevicePixels="True" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Background="Transparent" Margin="4,0,0,0">
                        <CheckBox.BorderBrush>
                            <SolidColorBrush Color="{DynamicResource Light-005}"/>
                        </CheckBox.BorderBrush>
                    </CheckBox>
                </Grid>
            </Border>
            <TabControl x:Name="MenuTC" Grid.ColumnSpan="2" TabStripPlacement="Left" BorderThickness="0,0,1,0" Style="{DynamicResource MenuTabControlStyle}" Margin="0" Padding="0">
                <TabControl.Background>
                    <SolidColorBrush Color="{DynamicResource Light-001}"/>
                </TabControl.Background>
                <TabControl.BorderBrush>
                    <SolidColorBrush Color="{DynamicResource Light-005}"/>
                </TabControl.BorderBrush>
                <TabItem x:Name="FavoriteTabItem" Width="60" Height="60" Margin="0" Style="{DynamicResource MenuTabItemStyle}" Background="Black" SnapsToDevicePixels="True">
                    <TabItem.Header>
                        <Grid>
                            <Viewbox Margin="3,3,0,0" Width="18" Height="18" Opacity="0.25" Visibility="Hidden">
